<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition" name="simplepdl">
  <figures
      name="Default">
    <figures
        xsi:type="gmfgraph:PolylineDecoration"
        name="GuidanceElementTargetDecoration"/>
    <figures
        xsi:type="gmfgraph:PolylineDecoration"
        name="ParameterWorkDefinitionTargetDecoration"/>
    <figures
        xsi:type="gmfgraph:PolylineDecoration"
        name="WorkSequenceDecoration"/>
    <descriptors
        name="WorkDefinitionFigure">
      <actualFigure
          xsi:type="gmfgraph:Ellipse"
          name="WorkDefinitionFigure">
        <layout
            xsi:type="gmfgraph:CenterLayout"/>
        <children
            xsi:type="gmfgraph:Label"
            name="WorkDefinitionMin_timeFigure"
            text=""/>
        <children
            xsi:type="gmfgraph:Label"
            name="WorkDefinitionMax_timeFigure"
            text=""/>
        <children
            xsi:type="gmfgraph:Label"
            name="WorkDefinitionNameFigure"
            text=""/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.0"/>
      <accessors
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.1"/>
      <accessors
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.2"/>
    </descriptors>
    <descriptors
        name="WorkSequenceFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"
          name="WorkSequenceFigure"
          targetDecoration="//@figures.0/@figures.2">
        <children
            xsi:type="gmfgraph:Label"
            name="WorkSequenceLinkTypeFigure"
            text=""/>
        <children
            xsi:type="gmfgraph:Label"
            name="WorkSequenceNameFigure"
            text=""/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.1/@actualFigure/@children.0"/>
      <accessors
          figure="//@figures.0/@descriptors.1/@actualFigure/@children.1"/>
    </descriptors>
    <descriptors
        name="GuidanceFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="GuidanceFigure">
        <layout
            xsi:type="gmfgraph:CenterLayout"/>
        <children
            xsi:type="gmfgraph:Label"
            name="GuidanceTextFigure"
            text=""/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.2/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="GuidanceElementFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"
          name="GuidanceElementFigure"
          targetDecoration="//@figures.0/@figures.0"/>
    </descriptors>
    <descriptors
        name="ResourceFigure">
      <actualFigure
          xsi:type="gmfgraph:RoundedRectangle"
          name="ResourceFigure">
        <layout
            xsi:type="gmfgraph:CenterLayout"/>
        <children
            xsi:type="gmfgraph:Label"
            name="ResourceNameFigure"
            text=""/>
        <children
            xsi:type="gmfgraph:Label"
            name="ResourceMarkingFigure"
            text=""/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.4/@actualFigure/@children.0"/>
      <accessors
          figure="//@figures.0/@descriptors.4/@actualFigure/@children.1"/>
    </descriptors>
    <descriptors
        name="ParameterFigure">
      <actualFigure
          xsi:type="gmfgraph:RoundedRectangle"
          name="ParameterFigure">
        <layout
            xsi:type="gmfgraph:CenterLayout"/>
        <children
            xsi:type="gmfgraph:Label"
            name="ParameterNbNeedsFigure"
            text=""/>
        <children
            xsi:type="gmfgraph:Label"
            name="ParameterNameFigure"
            text=""/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.5/@actualFigure/@children.0"/>
      <accessors
          figure="//@figures.0/@descriptors.5/@actualFigure/@children.1"/>
    </descriptors>
    <descriptors
        name="ParameterWorkDefinitionFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"
          name="ParameterWorkDefinitionFigure"
          sourceDecoration="//@figures.0/@figures.1"/>
    </descriptors>
  </figures>
  <nodes
      name="WorkDefinition"
      figure="WorkDefinitionFigure"/>
  <nodes
      name="Guidance"
      figure="GuidanceFigure"/>
  <nodes
      name="Resource"
      figure="ResourceFigure"/>
  <nodes
      name="Parameter"
      figure="ParameterFigure"/>
  <connections
      name="WorkSequence"
      figure="WorkSequenceFigure"/>
  <connections
      name="GuidanceElement"
      figure="GuidanceElementFigure"/>
  <connections
      name="ParameterWorkDefinition"
      figure="ParameterWorkDefinitionFigure"/>
  <labels
      name="WorkDefinitionMin_time"
      figure="WorkDefinitionFigure"
      accessor="//@figures.0/@descriptors.0/@accessors.0"/>
  <labels
      name="WorkDefinitionMax_time"
      figure="WorkDefinitionFigure"
      accessor="//@figures.0/@descriptors.0/@accessors.1"/>
  <labels
      name="WorkDefinitionName"
      figure="WorkDefinitionFigure"
      accessor="//@figures.0/@descriptors.0/@accessors.2"/>
  <labels
      name="WorkSequenceLinkType"
      figure="WorkSequenceFigure"
      accessor="//@figures.0/@descriptors.1/@accessors.0"/>
  <labels
      name="WorkSequenceName"
      figure="WorkSequenceFigure"
      accessor="//@figures.0/@descriptors.1/@accessors.1"/>
  <labels
      name="GuidanceText"
      figure="GuidanceFigure"
      accessor="//@figures.0/@descriptors.2/@accessors.0"/>
  <labels
      name="ResourceName"
      figure="ResourceFigure"
      accessor="//@figures.0/@descriptors.4/@accessors.0"/>
  <labels
      name="ResourceMarking"
      figure="ResourceFigure"
      accessor="//@figures.0/@descriptors.4/@accessors.1"/>
  <labels
      name="ParameterNbNeeds"
      figure="ParameterFigure"
      accessor="//@figures.0/@descriptors.5/@accessors.0"/>
  <labels
      name="ParameterName"
      figure="ParameterFigure"
      accessor="//@figures.0/@descriptors.5/@accessors.1"/>
</gmfgraph:Canvas>
